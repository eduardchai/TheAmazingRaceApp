@using Microsoft.AspNet.Identity;

@{
    ViewBag.Title = "Dashboard";
}

@if (User.Identity.GetMasterRoleName() == "Administrator")
{
    @Html.Partial("_Admin");
}
else if (User.Identity.GetMasterRoleName() == "Staff")
{
    @Html.Partial("_Staff");
}
else
{
    @Html.Partial("_Participant");
}